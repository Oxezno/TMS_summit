        background: url('../../images/loading.gif') no-repeat center center;
    }
</style>

<div id="pageloaddivA" hidden>
</div>

@Scripts.Render("~/bundles/jquery")

<script>

    var totalDayAttendance = 0;

    function setColorCellA(userid, day) {
        var color = '';
        var txtColor = 'white';
        var val = '';

        if (document.getElementById("chkAtt_" + + userid + day).checked == true) {
            $('#cell_' + userid + day).css('background', 'green');
            $('#cellLabel_' + userid + day).text('Present ✔');
            val = 'Yes';
        }
        if (document.getElementById("chkAtt_" + + userid + day).checked == false) {
            $('#cell_' + userid + day).css('background', '#C0392B');
            $('#cellLabel_' + userid + day).text('Absent ✘');
            val = 'No';
        }

        jQuery.ajax({
                type: 'POST',
                url: '@Url.Action("SaveTrainingAttendance", "TrainingCours")',
                data: { "uID": userid, "tID": @Model.Training.TrainingCourseID, "day": day, "attended": val, "group":'A' },
                success: function (data) {
                    toastr.success('Saved 😀');
                    //elementsYes = document.getElementsByClassName('yesOption');
                    $('#A_totalDay_' + day).html(parseInt(data))
                    $('#A_HtotalDay_' + day).html("("+parseInt(data)+")")

                },
                error: function (textStatus) {
                    toastr.error('Error: Something wrong!');
                }
            });

    }

    function fnReloadAttendanceA() {
        $('#pageloaddivA').show();

        var tcID = @ViewBag.TrainingCourseID;

        $.ajax({
            url: '@Url.Action("GetAttendanceListA", "TrainingCours")',
            data: { "id": tcID },
            success: function (data) {
                $('#pageloaddivA').hide();
                $("#attendanceA-list").html(data);
                toastr.success('Updating attendance list...');
            }
        });

    }

</script>