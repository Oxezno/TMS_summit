@model dynamic
@{
    Layout = null;

    string styleX = "pointer-events:none; ";
    if (Convert.ToString(Session["RoleName"]) == "Admin" || Convert.ToString(Session["RoleName"]) == "Super Admin" || Convert.ToString(Session["RoleName"]) == "Trainer")
    {
        styleX = "";
    }

    var num = 0;
}


<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#attendanceA1">1st Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA2">2nd Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA3">3rd Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA4">4th Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA5">5th Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA6">6th Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA7">7th Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA8">8th Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA9">9th Week</a></li>
    <li class=""><a data-toggle="tab" href="#attendanceA10">10th Week</a></li>

    @{
       
        <li class="pull-right">
            <a href="@Url.Action("AttendanceListExcelA", new { controller = "TrainingCours", id = (int)ViewBag.TrainingCourseID })" class="btn btn-primary btn-add" title="Click to Export" data-toggle="tooltip" >
                <i class="fa fa-file-excel-o"></i> Export
            </a>
        </li>

        if (Convert.ToString(Session["RoleName"]) == "Recruiter")
        {
            <li class="pull-right">
                <a class="btn btn-success btn-add" title="Refresh" data-toggle="tooltip" onclick="fnReloadAttendanceA()">
                    <i class="fa fa-refresh"></i> Refresh
                </a>
            </li>
        }
    }
</ul>

<div class="tab-content">
    <div id="attendanceA1" class="tab-pane fade in active">
        <div class="fullresponsivetable">
            <table class="table table-condensed">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Name</th>
                        <th>Last Name</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>Recruiter</th>

                        @{
                            //if(Model.Training.StartDate1)
                        }
                        @{
                            for (int i = 1; i <= 5; i++)
                            {
                                <th class="text-center">Day @i <div id="A_HtotalDay_@i">(@ViewData["A_TodayDay" + i])</div></th>
                            }
                        }

                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @{num = 1; }
                    @foreach (var item in Model.UsersA)
                    {
                        var date1 = "";

                        if (item.PaymentDate != null)
                        {
                            date1 = Convert.ToDateTime(item.PaymentDate).ToString("MM/dd/yyyy");
                        }

                    <tr>
                        <td data-title="Number">@num</td>
                        <td data-title="Fname">@item.WebsiteUser.FName</td>
                        <td data-title="LName">@item.WebsiteUser.LName</td>
                        <td data-title="Email">@item.WebsiteUser.Email</td>
                        <td data-title="Phone">@item.WebsiteUser.Phone</td>
                        <td data-title="Recruiter">@item.WebsiteUser.RecruiterName</td>


                        @{
                            for (int i = 1; i <= 5; i++)
                            {
                                var selectedChk = "";
                                var cellColor = "#C0392B";
                                var cellLabel = "Absent ✘";
                                foreach (var att in Model.Attendance)
                                {

                                    if (att.WebsiteUserID == item.WebsiteUser.WebsiteUserID && att.Day == i)
                                    {

                                        if (att.Attended == true)
                                        {
                                            selectedChk = "checked";
                                            cellColor = "green";
                                            cellLabel = "Present ✔";
                                            break;
                                        }

                                    }

                                }
                                <td class="text-center" id="cell_@item.WebsiteUser.WebsiteUserID@i" style="background-color:@cellColor">
                                    @{
                                        if (Convert.ToString(Session["RoleName"]) != "Recruiter")
                                        {
                                            <input type="checkbox" value="" id="chkAtt_@item.WebsiteUser.WebsiteUserID@i" onchange="setColorCellA(@item.WebsiteUser.WebsiteUserID, @i)" @selectedChk>
                                        }
                                    }
                                    <label id="cellLabel_@item.WebsiteUser.WebsiteUserID@i">@cellLabel</label>
                                </td>
                            }
                        }


                    </tr>
                    num++;
                    }

                    <tr class="text-center">
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td class="text-right"><b>Total</b></td>
                        <td><div id="A_totalDay_1">@ViewData["A_TodayDay1"]</div></td>
                        <td><div id="A_totalDay_2">@ViewData["A_TodayDay2"]</div></td>
                        <td><div id="A_totalDay_3">@ViewData["A_TodayDay3"]</div></td>
                        <td><div id="A_totalDay_4">@ViewData["A_TodayDay4"]</div></td>
                        <td><div id="A_totalDay_5">@ViewData["A_TodayDay5"]</div></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

